{
  "name": "trick-dog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/server/server",
    "dev": "concurrently 'npm:dev:client' 'npm:dev:server'",
    "dev:client": "vite",
    "dev:server": "nodemon --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/server/server.ts",
    "build": "tsc && vite build",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier --write \"./**/*.{ts,tsx}\"",
    "format-check": "prettier --list-different \"./**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-query-devtools": "^4.29.6",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/express-session": "^1.17.7",
    "@types/http-errors": "^2.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "connect-pg-simple": "^9.0.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "google-auth-library": "^8.8.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-intersection-observer": "^9.4.3",
    "react-router-dom": "^6.14.1",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@tanstack/eslint-plugin-query": "^4.29.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/google.accounts": "^0.0.7",
    "@types/node": "^20.3.0",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
